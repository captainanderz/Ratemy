@model RateMy.com.Models.StartPageViewModel
<div id="main">
    @foreach (var imagePropertys in Model.ImagesWithInfo)
    {
        <article class="thumb">
            <a href="@imagePropertys.FilePathContentFolder" class="image">
                @* Full image *@
                <img src="~/Content/images/thumbs_Template/01.jpg" alt="" /> @* Thumb image *@
            </a>
            <h2>By @imagePropertys.Name</h2>
            @* Upvote/Downvote area *@
        </article>
    }
</div>