@model RateMy.com.Models.StartPageViewModel
<div id="main">
    @foreach (var imagePropertys in Model.ImagesWithInfo)
    {
        <article class="thumb">
            <a href="@imagePropertys.FullImageFilePathContentFolder" class="image" > @* Full image *@
                <img src="@imagePropertys.ThumbImageFilePathContentFolder" alt=""/> @* Thumb image *@
            </a>
            <h2>By @imagePropertys.Name</h2>
            @* Upvote/Downvote area *@
        </article>
    }
</div>